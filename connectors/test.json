{
  "name": "testing",
  "config": {
    "connector.class": "io.confluent.connect.jdbc.JdbcSourceConnector",
    "connection.url": "jdbc:mysql://mysqldb:3306/kafka",
    "connection.user": "root",
    "connection.password": "root",
    "table.whitelist" : "employee",
    "mode": "incrementing",
    "incrementing.column.name": "emp_id",
    "topic.prefix": "",
    "key": "emp_id",
    "key.converter": "org.apache.kafka.connect.storage.StringConverter",
    "value.converter": "org.apache.kafka.connect.json.JsonConverter",
    "value.converter.schemas.enable": false,
    "connection.attempts": "3",
    "retry.backoff.ms": "5000",
    "transforms": "ksqlExtractString,ksqlCreateKey",
    "transforms.ksqlExtractString.type": "org.apache.kafka.connect.transforms.ExtractField$Key",
    "transforms.ksqlCreateKey.type": "org.apache.kafka.connect.transforms.ValueToKey",
    "transforms.ksqlCreateKey.fields": "emp_id",
    "transforms.ksqlExtractString.field": "emp_id"
  }
}

